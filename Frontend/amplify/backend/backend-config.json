{
  "function": {
    "classificate-image": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "save-checking": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "checkings",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "storage": {
    "checkings": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "api": {
    "cplabWebappApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "classificate-image",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "save-checking",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}